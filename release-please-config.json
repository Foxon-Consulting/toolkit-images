{
  "release-type": "simple",
  "changelog-path": "CHANGELOG.md",
  "bump-minor-pre-major": false,
  "bump-patch-for-minor-pre-major": false,
  "draft": false,
  "prerelease": false,
  "include-v-in-tag": false,
  "packages": {
    "base": {
      "package-name": "base-toolkit",
      "extra-files": ["base/docker-compose.yml"]
    },
    "devops": {
      "package-name": "devops-toolkit",
      "extra-files": ["devops/docker-compose.yml"]
    },
    "node": {
      "package-name": "node-toolkit",
      "extra-files": ["node/docker-compose.yml"]
    },
    "python": {
      "package-name": "python-toolkit",
      "extra-files": ["python/docker-compose.yml"]
    },
    "langchain": {
      "package-name": "langchain-toolkit",
      "extra-files": ["langchain/docker-compose.yml"]
    },
    "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
  }
}
